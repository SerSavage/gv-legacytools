{
  "root": ".",
  "clean_urls": false,
  "routes": {
    "/database": "/database/database/index.html",
    "/database/**": "/database/database/index.html",
    "/map/**": "/map/index.html",
    "/**": "/index.html"
  },
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=3600"
    },
    "/GVmaptiles/**": {
      "Cache-Control": "public, max-age=86400"
    },
    "/map/GVmaptiles/**": {
      "Cache-Control": "public, max-age=86400"
    }
  }
}
